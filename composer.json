{
    "name": "diviky/laravel-security",
    "description": "Provides security settings and login history",
    "keywords": [
        "laravel",
        "authentication",
        "log",
        "notification"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "bepsvpt/secure-headers": "^7.0",
        "geocoder-php/chain-provider": "^4.0",
        "geocoder-php/geoip2-provider": "^4.0",
        "matomo/device-detector": "^3.12",
        "pragmarx/firewall": "^2.2",
        "pragmarx/google2fa-laravel": "^2.0",
        "pragmarx/recovery": "^0.2",
        "bacon/bacon-qr-code": "^2.0",
        "diviky/bright": "^4.0|^5.0",
        "toin0u/geocoder-laravel": "^4.4"
    },
    "require-dev": {
        "diviky/code-analysis": "^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Diviky\\Security\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Diviky\\Security\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Diviky\\Security\\SecurityServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "phpro/grumphp": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
